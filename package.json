{
  "name": "phaser3-typescript-parcel-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for phaser3 with typescript and parcel",
  "repository": {
    "type": "git",
    "url": "https://github.com/softdevstory/phaser3-typescript-parcel-boilerplate"
  },
  "homepage": "http://p3.softdevstory.net/",
  "source": "src/index.html",
  "scripts": {
    "start": "npx parcel -p 8080",
    "build": "npx parcel build --dist-dir dist"
  },
  "author": "developer@softdevstory.net",
  "license": "MIT",
  "devDependencies": {
    "parcel": "^2.6.0",
    "parcel-reporter-clean-dist": "^1.0.4",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "phaser": "^3.55.2",
    "typescript": "^4.7.2"
  },
  "cleanDistFiles": [
    "!dist/CNAME",
    "!dist/.git"
  ],
  "staticFiles": {
    "staticPath": "public"
  }
}
